{
  // 项目名称，会在ESA控制台显示
  "name": "vme-kfc-jokes",
  
  // 安装依赖命令
  "installCommand": "npm install",
  
  // 构建命令（Next.js静态导出）
  "buildCommand": "npm run build",
  
  // 部署资源配置
  "assets": {
    // Next.js构建输出到dist目录
    "directory": "./dist",
    
    // 部署为服务器端渲染应用
    "notFoundStrategy": "proxy"
  },
  
  // 环境变量（可选）
  "env": {
    "NODE_VERSION": "20"
  }
}